<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

  <system.serviceModel>
    <services>
      <service name="SyndicationServiceLibrary.Feed" behaviorConfiguration="SyndicationServiceLibrary.serviceBehavior">
        <endpoint address="Feed" behaviorConfiguration="SyndicationServiceLibrary.endpointBehavior"
          binding="webHttpBinding" contract="SyndicationServiceLibrary.IFeed" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/SyndicationServiceLibrary/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="SyndicationServiceLibrary.serviceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <behavior name="SyndicationServiceLibrary.endpointBehavior">
          <webHttp/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>